	public class Level extends World
	{
		public var level:Array;
		private var c:Canvas = new Canvas(640, 480);
		private var walls:Array = new Array;
		private var slopeRs:Array = new Array;
		private var slopeLs:Array = new Array;
		private var time:Number = 0;
		private var cur_level:int = 0;
		[Embed(source = '../../assets/hline.png')] private const HLINE:Class;
		[Embed(source = '../../assets/vline.png')] private const VLINE:Class;
		[Embed(source = '../../assets/border.png')] private const BORDER:Class;
		private var b:Image;
		public function Level(l:int)
		{
			c.fill(new Rectangle(0, 0, 640, 480), 0x00FFFFFF, .4);
			var i:int;
			var vline:Image = new Image(VLINE);
			var hline:Image = new Image(HLINE);
			b = new Image(BORDER)
			for (i = 0; i < 640; i += 16)
			{
				c.drawGraphic(0, i, hline);
				c.drawGraphic(i, 0, vline);
			}
			
			gameData.Load = load_level;
			
			load_level(l);
		}
		
		public function load_level(lev:int = -1):void
		{
			time = 0;
			cur_level++;
			var l:int = cur_level;
			if (lev != -1)
			{
				l = lev;
			}
			removeAll();
			level = get_level_array(l);
			var i:int;
			for (i = 0; i < level.length; i++)
			{
				switch(level[i])
				{
				case 1:
					add(new Wall((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 2:
					add(new slopeR((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 3:
					add(new slopeL((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 4:
					add(new player((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 5:
					add(new lava((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 6:
					add(new goal((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, load_level));
					break;
				case 7:
					add(new bounce((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 8:
					add(new fall((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 9:
					add(new water((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 10:
					add(new conveyer((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, "right"));
					break;
				case 11:
					add(new conveyer((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, "left"));
					break;
				case 12:
					add(new ice((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 13:
					add(new grav((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 14:
					add(new push((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8));
					break;
				case 15:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, false, -1));
					break;
				case 16:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, false, 4));
					break;
				case 17:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, true, 4));
					break;
				case 18:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, false, 5));
					break;
				case 19:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, true, 5));
					break;
				case 20:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, false, 6));
					break;
				case 21:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, true, 6));
					break;
				case 22:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, false, 7));
					break;
				case 23:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, true, 7));
					break;
				case 24:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, false, 8));
					break;
				case 25:
					add(new moveP((i % 39) * 16 + 8, Math.floor(i / 39) * 16 + 8, true, 8));
					break;
				}
			}
			updateLists();
		}
		
		override public function update():void
		{
			super.update();
			if (time == 1)
			{
				getClass(Wall, walls);
				getClass(slopeR, slopeRs);
				getClass(slopeL, slopeLs);
				
				for each (var w:Wall in walls)
				{
					w.assignSprite();
				}
				
				for each (var s1:slopeL in slopeLs)
				{
					s1.assignSprite();
				}
				
				for each (var s2:slopeR in slopeRs)
				{
					s2.assignSprite();
				}
			}
		}
		
		override public function render():void 
		{
			time++;
			c.render(new Point(0, 0), new Point(0, 0));
			if (time > 1)
				super.render();
			b.render(new Point(0, 0), new Point(0, 0));
		}
		
